{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-restricted-imports": ["error", { "paths": ["next/head"] }],
    "react/no-unescaped-entities": [
      "error",
      {
        "forbid": [
          { "char": ">", "alternatives": ["&gt;"] },
          { "char": "}", "alternatives": ["&#125;"] }
        ]
      }
    ]
  }
}
